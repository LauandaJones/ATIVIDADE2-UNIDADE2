public class Aviao
{
	 //altitude inicial 10 Km e velocidade 900 Km/h
	 int altitude = 10;
	 int velocidade = 900;
	 
	 public Aviao (int altitude = 10, int velocidade = 900) {
	     this.altitude = altitude;
	     this.velocidade = velocidade;
	 }
	 public int ReduzirAltitude (int altitude) {
	     int Altitude = altitude - 1;
	     return Altitude;
	 }
	 public int ReduzirVelocidade (int velocidade) {
	     velocidade = velocidade - 100;
	     return velocidade;
	 }
}
